{
	"compilerOptions": {
		"target": "es2020",
		"strict": true,
		"preserveConstEnums": true,
		"sourceMap": false,
		"module": "es2015",
		"moduleResolution": "node",
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"outDir": "../dist/lambda"
	},
	"include": ["./src/**/*"],
	"exclude": ["./src/**/*.spec.ts"]
}
